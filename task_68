//Написать программу вычисления функции Аккермана с помощью рекурсии.
//Даны два неотрицательных числа m и n.
//m = 2, n = 3 -> A (m, n) = 9
//m = 3, n = 2 -> A (m, n) = 29

int m = входные числа("Введите M: ");
int n = входные числа("Введите N: ");

функция ввода kakkerman = Ack(m, n);

Консоль.Write($"Функция Аккермана = {functionAkkerman} ");

подтверждение ввода (int m, int n)
{
 если (m == 0) возвращает n + 1;
 иначе, если (n == 0) вернуть подтверждение (m - 1, 1);
 иначе верните подтверждение (m - 1, Ack(m, n - 1));
}

