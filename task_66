//Задать значения M и N. 
//Написать программу, которая найдёт сумму натуральных элементов в промежутке от M до N.
//M = 1; N = 15 -> 120
//M = 4; N = 8. -> 30

int m = входные числа("Введите M: ");
int n = входные числа("Введите N: ");
int temp = m;

если (m > n)
{
 m = n;
 n = временная;
}

Количество печатей (m, n, temp = 0);

аннулирует результат печати (int m, int n, int summ)
{
 сумма = сумма + n;
 если (n <= m)
    {
 Консоль.Write($"Сумма элементов = {summ} ");
 Возврат;
    }
 Распечатайте (m, n - 1, summ);
}

int InputNumbers (ввод строки)
{
 Консоль.Запись (ввод);
 вывод int = преобразование.В INT32(консоль.Строка чтения());
 возвращает результат;
